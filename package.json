{
  "name": "@jsweb/material-icons-base64",
  "version": "1.2.3",
  "description": "Material Icons compiled with base64 embedded font to CSS instead of link to font files",
  "scripts": {
    "major": "npm version major && npm run push",
    "minor": "npm version minor && npm run push",
    "patch": "npm version patch && npm run push",
    "prepare": "npm run build",
    "push": "git push && git push --tags",
    "lint": "prettier --write build.js",
    "build": "run-s build:*",
    "build:packs": "packs",
    "build:lint": "npm run lint",
    "build:css": "babel-node build.js"
  },
  "keywords": [
    "icons",
    "font",
    "material",
    "base64"
  ],
  "author": "Alex Bruno CÃ¡ceres <git.alexbr@outlook.com>",
  "repository": "git@github.com:jsweb/material-icons-base64.git",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@jsweb/packs": "^2.0.9",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "stylus": "^0.54.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && git add ."
    }
  },
  "main": "main.css",
  "@jsweb/packs": {
    "dir": "jsweb-packs",
    "web": {
      "source.styl": "https://fonts.googleapis.com/icon?family=Material+Icons",
      "source.woff2": "https://fonts.gstatic.com/s/materialicons/v52/flUhRq6tzZclQEJ-Vdg-IuiaDsNc.woff2"
    }
  }
}
